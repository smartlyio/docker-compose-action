build:
  runs-on: ubuntu-22.04
  steps:
  - uses: actions/checkout@v4
  - name: Set Node.js 20.x
    uses: actions/setup-node@v4
    with:
      node-version: 20.x
  - name: "Install"
    run: |
      npm install
  - name: "Prettier"
    run: |
      npm run format-check
  - name: "Lint"
    run: |
      npm run lint
  - name: "Test"
    run: |
      npm run test
  - name: "build"
    run: |
      npm run build
      npm run package
